{
  "cli": {
    "version": ">= 16.7.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true
      }
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "EXPO_METRO_ENABLE_DEBUG": "true"
      }
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "EXPO_METRO_ENABLE_DEBUG": "false"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
